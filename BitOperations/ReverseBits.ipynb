   "source": [
    "ret, power = 0,3\n",
    "\n",
    "n = 8\n",
    "\n",
    "while n:\n",
    "    ret += (n&1)<<power\n",
    "    n = n>>1\n",
    "    power -= 1\n",
    "    \n",
    "print(ret)"
   ]

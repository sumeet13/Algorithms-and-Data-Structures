   "outputs": [],
   "source": [
    "def findMinimumRecursively(root):\n",
    "    if root is None:\n",
    "        return\n",
    "    if root.getLeft() is None:\n",
    "        return root\n",
    "    return findMinimumRecursively(root.getLeft())"
   ]

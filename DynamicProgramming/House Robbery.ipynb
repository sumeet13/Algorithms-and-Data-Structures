   "source": [
    "def rob(nums):\n",
    "    r = [0] * len(nums)\n",
    "    r[0] = nums[0]\n",
    "    \n",
    "    print(r)\n",
    "    for i in range(1, len(nums)):\n",
    "\n",
    "        q = -1\n",
    "        for j in range(i - 1):\n",
    "            q = max(r[j], q)\n",
    "\n",
    "        r[i] = max(q + nums[i], r[i - 1])\n",
    "\n",
    "    return r\n",
    "\n",
    "\n",
    "rob([1, 2, 3, 1])"
   ]
